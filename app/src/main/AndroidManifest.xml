<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="socialmedia.prosperity.newecosocialmedia">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_google_play"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_google_play"
        android:supportsRtl="true"

        android:theme="@style/Theme.NewEcoSocialMedia">
        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <!-- MAIN represents that it is the Main Activity-->
                <action android:name="android.intent.action.MAIN" />
                <!-- Launcher Denotes that it will be the first launching activity-->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".UserProfileActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".SearchTeamActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".CreateTeamActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".AdminMainActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".CreatePostActivity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".HomeActivity"
            android:screenOrientation="portrait"
            />
    </application>



</manifest>